{
  "name": "tosdr.org",
  "subdomain": "tos-dr",
  "domains": [
    "tosdr.org",
    "www.tosdr.org"
  ],
  "scripts": {
    "start": "server.js",
    "compile": "./node_modules/.bin/grunt && cp -r api/ dist/",
    "deploy": "npm run compile",
    "publish": "git add . && git commit -m \"built\" && git push && git subtree push --prefix dist 5apps master",
    "compileandpublish": "npm run compile && npm run publish"
  },
  "version": "0.0.0-18",
  "repository": "git@github.com:tosdr/tosdr.org.git",
  "dependencies": {
    "imap": "*",
    "mailparser": "^2.7.7",
    "mongodb": "^3.5.4",
    "mustache": "^4.0.0",
    "node-static": "^0.7.11",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "*",
    "grunt-contrib-imagemin": "^3.1.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-svgmin": "^6.0.0",
    "load-grunt-tasks": "^5.1.0",
    "matchdep": "*"
  }
}
